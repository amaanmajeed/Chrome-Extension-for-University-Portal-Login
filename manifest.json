{
  "manifest_version": 2,
  "name": "Auto Login to University Portal",
  "description": "This extension automates login for university portal with username, password and captcha.",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },


  "content_scripts": [
    {
      "matches": ["https://online.umt.edu.pk/Account/Login?ReturnUrl=%2F*"],
      "js": ["content.js"]
    }
  ],

  "permissions": [
    "activeTab",
    "https://online.umt.edu.pk/"
  ]
}
